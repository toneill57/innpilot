{
  "name": "innpilot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run kill-port && next dev --turbopack",
    "dev:force": "npm run kill-all && next dev --turbopack",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "kill-all": "pkill -f 'next dev' 2>/dev/null || true && pkill -f 'node.*next' 2>/dev/null || true && npm run kill-port",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "validate-env": "node scripts/validate-env.js",
    "validate-env:test": "node scripts/validate-env.js --test-connections",
    "pre-deploy": "npm run validate-env && npm run lint && npm run build",
    "deploy": "npm run pre-deploy && vercel --prod",
    "populate-embeddings": "node scripts/populate-embeddings.js",
    "clear-database": "node scripts/clear-database.js",
    "test-performance": "node scripts/test-performance.js",
    "benchmark-detailed": "node scripts/detailed-benchmark.js",
    "setup-pgvector": "node scripts/setup-vector-function.js",
    "monitor": "node scripts/system-monitor.js",
    "ensure-server": "node scripts/ensure-server.js"
  },
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@langchain/community": "^0.3.56",
    "@langchain/core": "^0.3.77",
    "@langchain/openai": "^0.6.13",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "glob": "^11.0.3",
    "langchain": "^0.3.34",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "openai": "^5.21.0",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
