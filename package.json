{
  "name": "muva-chat",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-tenant chat platform with SIRE compliance for tourism businesses",
  "scripts": {
    "dev": "npm run kill-port && next dev --turbopack",
    "dev:force": "npm run kill-all && next dev --turbopack",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "kill-all": "pkill -f 'next dev' 2>/dev/null || true && pkill -f 'node.*next' 2>/dev/null || true && npm run kill-port",
    "prebuild": "npx tsx scripts/build-welcome-message.ts",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:setup": "tsx e2e/setup/test-database-setup.ts",
    "validate-env": "node scripts/validate-env.js",
    "validate-env:test": "node scripts/validate-env.js --test-connections",
    "pre-deploy": "npm run validate-env && npm run lint && npm run build",
    "populate-embeddings": "node scripts/populate-embeddings.js",
    "clear-database": "node scripts/clear-database.js",
    "test-performance": "node scripts/test-performance.js",
    "benchmark-detailed": "node scripts/detailed-benchmark.js",
    "setup-pgvector": "node scripts/setup-vector-function.js",
    "monitor": "node scripts/system-monitor.js",
    "ensure-server": "node scripts/ensure-server.js",
    "sync:motopress:bookings": "tsx scripts/sync-motopress-bookings.ts",
    "clean": "rm -rf .next node_modules/.cache data/temp logs/*.log && echo '✅ Cache limpiado'",
    "clean:deep": "npm run clean && rm -rf node_modules && echo '✅ Reinstalando dependencias...' && npm install"
  },
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@langchain/community": "^0.3.56",
    "@langchain/core": "^0.3.77",
    "@langchain/openai": "^0.6.13",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tiptap/react": "^3.6.6",
    "@tiptap/starter-kit": "^3.6.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.20",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.22",
    "fuse.js": "^7.1.0",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html2canvas": "^1.4.1",
    "jose": "^6.1.0",
    "langchain": "^0.3.34",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "openai": "^5.21.0",
    "pdfjs-dist": "^5.4.149",
    "pg": "^8.16.3",
    "puppeteer": "^24.23.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^9.1.0",
    "react-pdf": "^10.1.0",
    "recharts": "^3.2.1",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.18",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
